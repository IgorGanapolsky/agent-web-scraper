{
  "name": "Stripe Live Payments Workflow",
  "nodes": [
    {
      "id": "webhook_trigger",
      "name": "Stripe Webhook",
      "type": "n8n-nodes-base.webhook",
      "position": [240, 300],
      "parameters": {
        "path": "stripe-live-payments",
        "httpMethod": "POST",
        "authentication": "headerAuth"
      },
      "webhookId": "live-stripe-payments"
    },
    {
      "id": "verify_stripe_signature",
      "name": "Verify Stripe Webhook Signature", 
      "type": "n8n-nodes-base.function",
      "position": [460, 300],
      "parameters": {
        "functionCode": "// Stripe webhook verification logic"
      }
    },
    {
      "id": "process_payment_event",
      "name": "Process Payment Event",
      "type": "n8n-nodes-base.function", 
      "position": [680, 300],
      "parameters": {
        "functionCode": "// Payment processing logic"
      }
    }
  ],
  "connections": {
    "webhook_trigger": {
      "main": [
        [
          {
            "node": "verify_stripe_signature",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "verify_stripe_signature": {
      "main": [
        [
          {
            "node": "process_payment_event", 
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  }
}